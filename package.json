{
  "name": "template-project",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "check:astro": "astro check",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "// Linting": "",
    "lint:stylelint": "stylelint \"**/*.{svelte,astro,css}\" '!./.db'",
    "lint:biome": "pnpm biome lint .",
    "lint": "pnpm lint:biome && pnpm lint:stylelint",
    "// Formatting": "",
    "format:prettier": "prettier --write --plugin prettier-plugin-astro --plugin prettier-plugin-svelte . \"!./.db\"",
    "format:biome": "pnpm biome format . --write",
    "format": "pnpm format:biome && pnpm format:prettier",
    "// Fixes": "",
    "lint:stylelint:fix": "stylelint --fix \"**/*.{svelte,astro,css}\" '!./.db'",
    "lint:biome:fix": "pnpm biome lint . --fix",
    "// Checks": "",
    "check:format:prettier": "prettier --check --plugin prettier-plugin-astro --plugin prettier-plugin-svelte . \"!./.db\"",
    "check:format:biome": "pnpm biome format .",
    "check:format": "pnpm check:format:biome && pnpm check:format:prettier",
    "check": "pnpm check:astro && pnpm check:format && pnpm lint"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.2.1",
    "@astrojs/node": "^9.1.0",
    "@astrojs/react": "^4.2.0",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^7.0.4",
    "@astrojs/tailwind": "^6.0.0",
    "@fontsource-variable/dancing-script": "^5.2.5",
    "@fontsource-variable/playfair-display": "^5.2.5",
    "@fontsource/tangerine": "^5.2.5",
    "@supabase/supabase-js": "^2.48.1",
    "astro": "^5.3.0",
    "astro-robots-txt": "^1.0.0",
    "autoprefixer": "^10.4.20",
    "cloudflare": "^4.2.0",
    "core-js": "^3.40.0",
    "file-saver": "^2.0.5",
    "flatpickr": "^4.6.13",
    "flyonui": "^1.3.0",
    "postcss": "^8.5.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "svelte": "^5.20.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "wrangler": "^4.19.1"
  }
}
